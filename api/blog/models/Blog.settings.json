{
  "connection": "default",
  "collectionName": "blogs",
  "info": {
    "name": "blog",
    "description": "Blog post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "title": {
      "default": "",
      "required": true,
      "unique": true,
      "type": "string"
    },
    "body": {
      "required": true,
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    }
  }
}